     SET
     summary = #{summary},
     score = #{score},
     score_rate = #{scoreRate},
     actual_total = #{actualTotal},
     impt_item_deny_num = #{imptItemDenyNum},
     na_num = #{naNum},
     summary_img = #{summaryImg},
     modify_time = #{modifyTime}
     WHERE front_plan_id = #{frontPlanId} AND form_main_id = #{formMainId}
  </update>
